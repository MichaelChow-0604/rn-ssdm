{
  "expo": {
    "name": "rn-ssdm",
    "slug": "rn-ssdm",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mehchow.rnssdm",
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-document-picker",
      [
        "expo-asset",
        {
          "assets": [
            "./assets/docs/mywill.docx",
            "./assets/docs/ssdm.pdf",
            "./assets/docs/rikki.jpg"
          ]
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/app_icon.png"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "108e1b73-3de9-4d92-83d4-d94089035681"
      }
    }
  }
}
